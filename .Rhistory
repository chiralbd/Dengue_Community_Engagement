recode_val <- function(x){
ifelse(x=="Yes", 1,0)
}
data2 <- data %>%
select(14:23) %>%
mutate_all(recode_val) %>%
mutate(Total_Practice_Score = rowSums(across(where(is.numeric)))) %>%
